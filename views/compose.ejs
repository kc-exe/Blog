<%- include("partials/header"); -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="postTitle" class="form-control" placeholder="Enter Post Title">
    <label>Post</label>
    <textarea name="postBody" class="form-control" rows="5" cols="30" placeholder="Enter your Post"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include("partials/footer"); -%>
